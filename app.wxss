/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background-color: white;
} 

/* 主界面载体 */
.mainBackgroundView {
  width: 100%;
  height: 100%;
  background-color: #f0f0f0;
}

/* 主界面头部图片 */
.mainHeaderImageView {
  width: 100%;
}
.mainHeaderImageView image {
  width: 100%;
  display: block; /* 不加底部会留白 */
}

/* 主界面列表 */
.main_tableView {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  box-sizing: border-box;
  background-color: #F0F0F0;
}
/* 主界面列表cell */
.main_tableView_cell {
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  height: 140rpx;
  background-color: white;
  border-radius: 10rpx;
}
/* 主界面列表cell图片 */
.main_tableView_cell_image {
  margin-top: 10rpx;
  margin-left: 20rpx;
  width: 120rpx;
  height: 120rpx;
  float: left;
}
.main_tableView_cell_image image {
  width: 100%;
  height: 100%;
}
/* 主界面cell标题 */
.main_tableView_cell_title {
  position: relative;
  top: 32rpx;
  left: 20rpx;
  height: 30rpx;
  font-size: 16px;
  font-weight: bold;
  color: #333333;
  display: block;
}
/* 主界面cell描述文字 */
.main_tableView_cell_content {
  position: relative;
  top: 50rpx;
  left: 20rpx;
  height: 30px;
  font-size: 14px;
  color: #999999;
}