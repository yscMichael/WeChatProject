.receptionListCell-main{
  background-color:#fff;
  border: 1rpx #fff solid;
}

.receptionList-line{
  background-color: #F2F2F4;
  width: 100%;
  height: 10rpx;
}

.receptionList-content{
  background-color:#fff;
  /* 自适应高度 */
  /* https://www.cnblogs.com/dongguapifly/p/7565882.html */
  overflow: hidden;
}

.receptionList-leftItem{
  margin: 0rpx 5rpx 0rpx 0rpx;
  padding: 10rpx 5rpx 10rpx 20rpx;
  float: left;
}

.receptionList-rightItem{
  float: right;
  width: 53px;
  height: 42px;
}

.receptionList-name{
  font-size: 32rpx;
  font-weight: bold;
  color: #555;
  text-align: left;
}

.receptionList-age{
  margin: 10rpx;
  font-size: 24rpx;
  color: #4D81EE;
  border: 1px solid #4D81EE;
  border-radius: 5px;
  text-align: left;
}

.receptionList-time{
  margin-top: 10rpx;
  margin-right: 10rpx;
  font-size: 26rpx;
  color: #888888;
  text-align: left;
}

.receptionList-number{
  margin-top: 10rpx;
  margin-right: 10rpx;
  font-size: 26rpx;
  color: #888888;
  text-align: left;
}