Page{
  background-color: red;
  width: 100%;
  height: 100%;
}

.main-view{
  background-color: green;
  width: 100%;
  height: 100%;
}

/* 搜索框 */
.search-box{
  background-color: #fff;
  left: 0;
  top: 0;
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items: center;
}

.searchBox-leftItem{
  width: 100%;
  height: 80%;
  margin: 10rpx;
  display: flex;
  flex-direction:row;
  align-items: center;
  background-color: #EDEDED;
  border-radius:5px;
}

.seach-icon{
  width: 30rpx;
  height: 30rpx;
  margin-left: 15rpx;
}

.search-input{
  width: 100%;
  height: 100%;
  margin-left:10rpx;
  font-size: 12px;
}

.noData-view{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #eee;
}

.noData-view image{ 
  width: 237rpx;
  height: 245rpx;
}