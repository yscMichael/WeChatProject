.main-view{
  background-color: #eee;
  width: 100%;
  height: 100%;
}

.noData-view{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #eee;
}

.noData-view image{ 
  width: 237rpx;
  height: 245rpx;
}

.head-view{
  display: flex;
  align-items: center;
  background-color: #F1F0F5;
}

.head-view image{
  width: 12px;
  height: 12px;
  margin-left: 10px;
}

.head-view view{
  color: #848483;
  font-size: 14px;
  margin-left: 5px;
}